{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/frontend/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "backend/api/app.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task"
  }
}
